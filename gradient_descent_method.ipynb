   "source": [
    "import time\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "6da0ad20",
   "metadata": {},
   "outputs": [],
   "source": [
    "def f(x):\n",
    "    return x*x - 5*x +5\n",
    "def df(x):\n",
    "    return 2*x - 5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "44406072",
   "metadata": {},
   "outputs": [],
   "source": [
    "N = 20 #number of iteratioin\n",
    "xx = 0 #begin value\n",
    "lmd = 0.1 #learning rate"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "73064369",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Значение х для построения графика"
    "x_plt = np.arange(0, 5.0, 0.1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "1f79f88d",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Значение ф-ии f(x) для построения графика"
    "f_plt = [f(x) for x in x_plt]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "2d48b45a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2.5021009052699092\n"
     ]
    },
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "#switch on interactive mode of plot's display\n",
    "plt.ion()\n",
    "fig, ax = plt.subplots() #create fig and axes\n",
    "ax.grid(True) #display grid on plot\n",
    "ax.plot(x_plt, f_plt)\n",
    "point = ax.scatter(xx, f(xx), c='red')\n",
    "\n",
    "mn = 100\n",
    "for i in range(N):\n",
    "    lmd = 1 / min(i + 1, mn)\n",
    "    xx = xx - lmd*np.sign(df(xx)) #change argument on current iteration\n",
    "    \n",
    "    point.set_offsets([xx, f(xx)]) # display new position of point\n",
    "    \n",
    "    #redraw plot and set delay 20ms\n",
    "    fig.canvas.draw()\n",
    "    fig.canvas.flush_events()\n",
    "    time.sleep(0.01)\n",
    "    \n",
    "plt.ioff()\n",
    "print(xx)\n",
    "ax.scatter(xx, f(xx), c='blue')\n",
    "plt.show()"
   ]
  }
 ],
